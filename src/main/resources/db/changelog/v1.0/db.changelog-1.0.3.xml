    <changeSet id="insert event type template data" author="Ngai Elizabeth" context="test">
        <insert tableName="event_type_template">
            <column name="id" value="1"/>
            <column name="type" value="NOMINATIONS"/>
            <column name="template" value="{1} will be nominated this week"/>
            <column name="description" value="Predict the housemate that will be nominated this week"/>
            <column name="housemate_num" value="1"/>
        </insert>
        <insert tableName="event_type_template">
            <column name="id" value="2"/>
            <column name="type" value="EVICTIONS"/>
            <column name="template" value="{1} will be evicted"/>
            <column name="description" value="Predict the housemate that will be evicted this week"/>
            <column name="housemate_num" value="1"/>
        </insert>
        <insert tableName="event_type_template">
            <column name="id" value="3"/>
            <column name="type" value="HEAD_OF_HOUSE"/>
            <column name="template" value="{1} will be the Head of House"/>
            <column name="description" value="Predict the housemate that will be the Head of House this week"/>
            <column name="housemate_num" value="1"/>
        </insert>
        <insert tableName="event_type_template">
            <column name="id" value="4"/>
            <column name="type" value="WINNER"/>
            <column name="template" value="{1} will be the ultimate winner"/>
            <column name="description" value="Predict the housemate that will be the ultimate winner"/>
            <column name="housemate_num" value="1"/>
        </insert>
        <insert tableName="event_type_template">
            <column name="id" value="5"/>
            <column name="type" value="DISQUALIFICATION"/>
            <column name="template" value="{1} will be disqualified"/>
            <column name="description" value="Predict the housemate that will be disqualified"/>
            <column name="housemate_num" value="1"/>
        </insert>

        <insert tableName="event_type_template">
            <column name="id" value="6"/>
            <column name="type" value="NOMINATIONS"/>
            <column name="template" value="{1} and {2} will be nominated this week"/>
            <column name="description" value="Predict the housemates that will be nominated this week"/>
            <column name="housemate_num" value="2"/>
        </insert>
    </changeSet>

